{
  "name": "redux-flash",
  "version": "2.0.3",
  "description": "Redux action creators for displaying flash messages",
  "main": "lib/index.js",
  "scripts": {
    "start": "yarn run build:development",
    "build": "mkdir -p lib && babel src --out-dir lib --no-comments --minified",
    "build:development": "mkdir -p lib && babel src --watch --out-dir lib",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "prepublish": "yarn run lint && yarn run clean && yarn run build",
    "test": "jest",
    "size": "yarn build && size-limit",
    "test:coverage": "jest --coverage",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": "launchpadlab/redux-flash",
  "keywords": [
    "redux",
    "flash",
    "message",
    "flash message",
    "alert"
  ],
  "author": "dpikt",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib/"
  ],
  "devDependencies": {
    "@launchpadlab/babel-preset": "^1.0.0",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "codeclimate-test-reporter": "^0.4.1",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^19.0.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.5.4",
    "size-limit": "^1.0.0"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "redux-actions": "^2.6.5",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "redux": "^4.0.0 || ^3.0.0"
  }
}
